<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>

  <PropertyGroup>
    <GenerateAssemblyInfo>true</GenerateAssemblyInfo>
    <Version>2.1.0.0</Version>
    <AssemblyVersion>2.1.0.0</AssemblyVersion>
    <FileVersion>2.1.0.0</FileVersion>
    <Company>UselessExtras</Company>
    <Product>s3i</Product>
    <PackageId>s3iLib</PackageId>
    <NeutralLanguage>en</NeutralLanguage>
    <Authors>Oleg Boulanov</Authors>
    <PackageProjectUrl>http://github.com/OlegBoulanov/s3i</PackageProjectUrl>
    <RepositoryUrl>http://github.com/OlegBoulanov/s3i</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AWSSDK.S3" Version="3.3.104.34" />
    <PackageReference Include="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="2.9.3" />
    <PackageReference Include="Newtonsoft.Json">
      <Version>12.0.2</Version>
    </PackageReference>
  </ItemGroup>
  
