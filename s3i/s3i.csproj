  <PropertyGroup>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <IsPublishable>true</IsPublishable>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="AWSSDK.S3" Version="3.3.106.6" />
    <PackageReference Include="AWSSDK.SimpleSystemsManagement" Version="3.3.106.6" />
    <PackageReference Include="AWSSDK.SecurityToken" Version="3.3.103.13" />
    <PackageReference Include="System.Text.Json" Version="4.6.0" />
    <!--PackageReference Include="Newtonsoft.Json" Version="12.0.2" /-->
  </ItemGroup>
  
  <ItemGroup>
    <ProjectReference Include="..\s3iLib\s3iLib.csproj" />
  </ItemGroup>
  
</Project>
